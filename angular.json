{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pbooth": {
      "root": "",
      "sourceRoot": "pbooth",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/pbooth",
            "index": "pbooth/index.html",
            "main": "pbooth/main.ts",
            "polyfills": "pbooth/polyfills.ts",
            "tsConfig": "pbooth/tsconfig.app.json",
            "assets": [
              "pbooth/assets",
              "pbooth/manifest.json"
            ],
            "styles": [
          		"node_modules/slick-carousel/slick/slick.scss",
    		    "node_modules/slick-carousel/slick/slick-theme.scss",
				"pbooth/styles.css"
            ],
            "scripts": [
                "pbooth/assets/js/jquery.min.js",
		        "node_modules/slick-carousel/slick/slick.min.js",
                "pbooth/assets/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "pbooth/environments/environment.ts",
                  "with": "pbooth/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": false,
              "ngswConfigPath": "pbooth/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pbooth:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pbooth:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pbooth:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "pbooth/test.ts",
            "polyfills": "pbooth/polyfills.ts",
            "tsConfig": "pbooth/tsconfig.spec.json",
            "karmaConfig": "pbooth/karma.conf.js",
            "styles": [
              "pbooth/styles.css"
            ],
            "scripts": [],
            "assets": [
              "pbooth/favicon.ico",
              "pbooth/assets",
              "pbooth/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "pbooth/tsconfig.app.json",
              "pbooth/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "pbooth-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "pbooth:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "pbooth:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "pclub": {
      "root": "",
      "sourceRoot": "pclub",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/pclub",
            "index": "pclub/index.html",
            "main": "pclub/main.ts",
            "polyfills": "pclub/polyfills.ts",
            "tsConfig": "pclub/tsconfig.app.json",
            "assets": [
              "pclub/assets"
            ],
            "styles": [
              "node_modules/animate.css/animate.min.css",
              "pclub/styles.css"
            ],
            "scripts": [
              "pclub/assets/js/jquery.min.js",
              "pclub/assets/js/bootstrap.min.js",
              "pclub/assets/js/slick.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "pclub/environments/environment.ts",
                  "with": "pclub/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pclub:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pclub:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pclub:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "pclub/test.ts",
            "polyfills": "pclub/polyfills.ts",
            "tsConfig": "pclub/tsconfig.spec.json",
            "karmaConfig": "pclub/karma.conf.js",
            "styles": [
              "pclub/styles.css"
            ],
            "scripts": [],
            "assets": [
              "pclub/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "pclub/tsconfig.app.json",
              "pclub/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "pclub-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "pclub:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "pclub:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "pbooth"
}

/* start of section_box css */
.section_box{padding:3rem 0 4rem; position:relative;}
.content { padding-bottom: 20px; }
/********** start of mainBanner css ********/
.mainBanner{position: relative; padding: 0;}
.mainBanner figure{width: 100%; margin-bottom: 0;}
.mainBanner figure img{width: 100%;}
.mainBanner .mainBannerContent{position: absolute; top: 1rem; left: 0; width: 100%; text-align: center; z-index: 2;}
.mainBanner .mainBannerContent h1{font-size: 6rem; color: #ffffff; text-transform: uppercase; line-height: 1.2; letter-spacing: .01rem; font-weight: bold;}
.mainBanner .mainBannerContent h1 span{display: block; font-size: 3.2rem; color: #fcd504;}

/******* start of saleEndsContent *******/
.saleEndsContent{position: absolute; top: -8rem; left: 50%; transform: translateX(-50%); text-align: center;}
.saleEndsContent h5{font-size: 2.5rem; color: #3b4e76; margin: 0; letter-spacing: .03rem;}
.saleEndsContent h5 span{font-size: 2.9rem; color: #ed1b62; display: block; padding-top: .8rem; line-height: .5;}

/********** start of offerHeading css ********/
.offerHeading.bottomImg{margin-top: 3rem; padding-top: 1.5rem;}
.offerHeading h1{text-align:center; font-size:1.2rem; font-weight:normal; text-transform:uppercase; margin:0; letter-spacing:.3rem; color:#3b4e76;}
.offerHeading h1 span{display:block; font-size:5rem; margin-bottom:2rem; font-weight:bold; letter-spacing:normal; line-height:1.3; color:#3b4e76; letter-spacing:.2rem;}
.offerHeading.productHeading h1{color: #ffffff;}
.offerHeading.productHeading h1 span{color: #ffffff; text-shadow: -1px 1px 0 #fcd504, 1px 1px 0 #fcd504, 1px -1px 0 #fcd504, -1px -1px 0 #fcd504;}


/********** start of product_detail css ************/
.product_listing{padding:0 .8rem 0; margin-top:2rem; display: flex; flex-wrap: wrap;}
.product_listing .list_div{margin-bottom: 2rem;}
.product_listing .list_div .product-grid3 {text-align: center; position: relative; height: 100%; background: transparent; box-shadow: 0 .3rem .5rem rgba(0,0,0,0.5); border:.1rem solid #ee9591; padding-bottom: 0.5rem; display: flex; flex-direction: column; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-image3 {position: relative; padding:1rem 0;}
.product_listing .list_div .product-grid3 .product-image3 .image_part {display: block; width:100%; background:none; border:none; padding:0; cursor:pointer; opacity:1; transform:scale(1); transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-image3 .image_part img {width: 25rem; height: auto; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .productTag{position: absolute; right: 0; top: -.1rem; z-index: 1; padding: .1rem .3rem .2rem; color: #ffffff; background: #ee9591; font-size: 1.2rem; text-transform: uppercase;}

/**************** product-content css *************************/
.product_listing .list_div .product-grid3 .product-content {padding: .5rem 1rem; text-align: center;}
.product_listing .list_div .product-grid3 .product-content .title {line-height: 0.2; text-align: center; border: none; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-content .title span{color: #ffffff; font-size: 2.3rem; font-weight: 300; letter-spacing: 0; display: inline-block; position: relative;}
.product_listing .list_div .product-grid3 .product-content .title span:before{content: ""; position: absolute; top: 0; left: 0; display: block; width: 100%; height: .09rem; transform: rotate(-10deg); background: #ee9591;}
.product_listing .list_div .product-grid3 .product-content .product_price {color: #ee9591; font-size: 2.9rem; font-weight: 500; display: block; padding:.2rem 0 .5rem; line-height: 1.2;}
.product_listing .list_div .product-grid3 .product-content .product_price span {font-size: 1.4rem;}
.product_listing .list_div .product-grid3 .product-content .product_off{color: #ffffff; font-size: 2rem; font-weight: 300; letter-spacing: 0; display: inline-block; position: absolute; left: 50%; top: -3.5rem; transform: translateX(-50%); margin: 0; text-transform: uppercase; border:none;}
.product_listing .list_div .product-grid3 .product-content a{height: 3rem; line-height: 2.7rem; padding: 0 25px;}

/************ start of hover ****************/
.product_listing .list_div .product-grid3:hover{box-shadow:0 0 2.5rem rgba(0,0,0,0.2);}
.product_listing .list_div .product-grid3:hover .product-content .title .title_content,
.product_listing .list_div .product-grid3:focus .product-content .title .title_content,
.product_listing .list_div .product-grid3:active .product-content .title .title_content {color: #3b4e76;}
/********** end of product_detail css ************/


/****** start of chooseGenderDiv popup css *************/
.chooseGenderPopup, .chooseGenderPopup .confirm_popup_div{background: #3b4e76;}
.global_popup .modal-content.chooseGenderPopup .modal-header{padding: 1.2rem 2.5rem; display: block; text-align: center;}
.global_popup .modal-content.chooseGenderPopup .modal-header h4{color: #ffffff; border-color: transparent; text-shadow: -1px 1px 0 #ee9591, 1px 1px 0 #ee9591, 1px -1px 0 #ee9591, -1px -1px 0 #ee9591; font-size: 3.2rem; text-transform: uppercase; letter-spacing: .1rem;}
.chooseGenderDiv{display: flex; flex-direction: row; justify-content: space-around; align-items: center; padding-bottom: 2rem;}
.clickGenderTag{display:block;}
.chooseGenderDiv .clickGender{display: flex; flex-direction: column; align-items: center; color: #ee9591; box-shadow: 0 .3rem .5rem rgba(0,0,0,0.5); border:.1rem solid #ee9591; padding: .5rem .3rem; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.chooseGenderDiv .clickGender .iconGender{font-size: 2.3rem;}
.chooseGenderDiv .clickGender .iconGender img{width: 4rem;}
.chooseGenderDiv .clickGender .iconText{font-size: 1.9rem; display: inline-flex; padding-top: .6rem; text-transform: capitalize;}
.chooseGenderDiv .clickGender:hover{color: #ffffff; border-color: #ffffff; box-shadow: none;}

/*********** start of claimBtn css **********/
.claimBtn{background: #3b4e76; padding: .3rem .5rem; text-align: center; cursor: pointer; position: fixed; bottom: 0; left: 0; width: 100%; z-index: 2;}
.claimBtn p{font-size: 2rem; color: #ffffff; margin: 0; text-transform: uppercase; letter-spacing: .01rem;}
.claimBtn p span{font-size: 3.5rem; line-height: 1.2; color: #fcd504; display: block; font-weight: bold; margin-top: -.3rem;}


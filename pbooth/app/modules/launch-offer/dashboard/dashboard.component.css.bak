@media (max-width:767px){
	/* start of brief_subscription_box css */
	.section_box{padding:1rem 0 4rem;}
	.section_box .row{padding-top: .5rem;}
	
	/********** start of mainBanner css ********/
	.mainBanner .mainBannerContent{top: 2.5rem;}
	.mainBanner .mainBannerContent h1{font-size: 2.8rem; line-height: 1.1;}
	.mainBanner .mainBannerContent h1 span{font-size: 2rem; padding-top: .3rem;}
	
	/******* start of saleEndsContent *******/
	.saleEndsContent{top: -4rem;}
	.saleEndsContent h5{font-size: 1.9rem;}
	.saleEndsContent h5 span{font-size: 2.2rem; padding-top: .5rem;}
	
	/********** start of product_detail css ************/
	.product_listing{padding: 0; margin-top: 0rem;}
	.product_listing .list_div{padding: 0 .2rem; margin-bottom: 1rem; margin-top: 1rem;}
	.product_listing .list_div .product-grid3{justify-content: space-between; align-items: center; padding: 1rem .3rem 0;}
	/*.product_listing .list_div:not(:last-child) .product-grid3{border-bottom: .1rem solid #ee9591;}
	.product_listing .list_div:first-child .product-grid3{padding-top: 0;}*/
	.product_listing .list_div .product-grid3 .product-image3{padding: 0;}
	.product_listing .list_div .product-grid3 .product-image3 .image_part img{width: 10rem;}
	.product_listing .list_div .product-grid3 .product-content{padding: .5rem 0;}
	.product_listing .list_div .product-grid3 .product-content .title span{font-size: 1.6rem;}
	.product_listing .list_div .product-grid3 .product-content .product_price{font-size: 1.6rem; line-height: 1;}
	.product_listing .list_div .product-grid3 .product-content .product_price span{font-size: 1rem;}
	.product_listing .list_div .product-grid3 .product-content .product_size mark{padding: .6em .3rem; font-size: .7rem; box-shadow: 0 0.2rem 0.4rem rgba(0,0,0,0.3);}
	.product_listing .list_div .product-grid3 .product-content .product_gifting{font-size: .8rem; padding: 0.6rem 0 0;}
	.product_listing .list_div .product-grid3 .product-content .product_off{font-size: 1.5rem; white-space: nowrap; top: -3rem;}
	.product_listing .list_div .product-grid3 .product-content a{height: 2.2rem; line-height: 1.8rem; font-size: 1.1rem;}
	.product_listing .list_div .product-grid3 .productTag{right: -.1rem; top: -.1rem; font-size: .9rem;}
	
	/********** start of offerHeading css ********/
	.offerHeading h1{font-size: 1.7rem; letter-spacing: 0.1rem;}
	.offerHeading h1 span{font-size: 2.4rem; margin-bottom: 1.5rem; margin-top: .2rem; letter-spacing: 0;}
	
	/****** start of chooseGenderDiv popup css *************/
	.global_popup .modal-content.chooseGenderPopup .modal-body{padding: 1rem 0 0;}
	.chooseGenderDiv .clickGender .iconGender img{width: 3rem;}
	.chooseGenderDiv .clickGender .iconText{font-size: 1.3rem;}
	.global_popup .modal-content.chooseGenderPopup .modal-header{padding: 1.2rem 1.5rem;}
	.global_popup .modal-content.chooseGenderPopup .modal-header h4{font-size: 1.8rem;}
	
	/*********** start of claimBtn css **********/
	.claimBtn p{font-size: 1.3rem;}
	.claimBtn p span{font-size: 2.2rem;}
}